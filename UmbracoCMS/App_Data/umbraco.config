      <tinTucItem id="1088" key="8334efed-fdec-473c-b365-8b71b17c9eed" parentID="1074" level="3" creatorID="0" sortOrder="0" createDate="2017-12-18T14:16:44" updateDate="2017-12-19T09:47:05" nodeName="Các Địa Điểm Du lịch Đà Nẵng – Bạn Không Thể Bỏ Qua - Phần 2" urlName="cac-dia-diem-du-lich-da-nang-ban-khong-the-bo-qua-phan-2" path="-1,1071,1074,1088" isDoc="" nodeType="1080" creatorName="wilson" writerName="wilson" writerID="0" template="1079" nodeTypeAlias="tinTucItem" isPublished="true">
